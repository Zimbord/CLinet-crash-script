local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Orion Example", HidePremium = false, SaveConfig = true, ConfigFolder = "Orion"})

local basicTab = Window:MakeTab({
	Name = "basic",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local basicSection = basicTab:AddSection({
	Name = "basic"
})

basicSection:AddButton({
	Name = "Crash serwer",
	Callback = function()
        local Players = game:GetService("Players")

        local LocalPlayer = Players.LocalPlayer
        local Character = LocalPlayer.Character
        local Humanoid = Character:WaitForChild("Humanoid")
        
        local Animation = Instance.new("Animation", Character)
        Animation.AnimationId = "rbxassetid://84315373"
        
        local Track = Humanoid:LoadAnimation(Animation)
        Track:Play()
    end
})
